#include<stdio.h>

void nhap(int a[], int n)
{
	for(int i = 0; i < n; i++)
	{
		printf("\nNhap a[%d]", i);
		scanf("%d", &a[i]);
		
	}
}

void xuat(int a[], int n)
{
	for(int i = 0; i < n; i++)
	{
		printf("%d", a[i]);
	}
}
void bubblesort(int a[], int n)
{
	for(int i = 0; i < n - 1; i++)
	for(int j = 0; j = n - 1; j--)
	if(a[j] < a[j - 1])
	{
		int x = a[j];
		a[j] = a[j - 1];
		a[j - 1] = x
	}
}

int main()
{
	int a[100];
	int n;
	printf("\nNhap so phan tu: ");
	scanf("%d", &n);
	nhap(a, n);
	printf("\nMang sau khi sap xep la: ");
    xuat(a, n);
	bubblesort(a, n);
}
